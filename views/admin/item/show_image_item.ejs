<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
  <div class="row">
    <% for (let x = 0; x < item.imageId.length; x++){ %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-header py-3">
          <h6 class="m-0 font-weight-bold text-primary">Show Image Item</h6>
        </div>
        <div class="card-body">
          <img src="http://localhost:3000/<%= item.imageId[x].imageUrl %>" alt="image" style="width: 100%; display: block" />
        </div>
      </div>
    </div>
    <% } %>
  </div>
  <a href="/admin/item" class="btn btn-warning btn-sm">Back</a>
</div>
